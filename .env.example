DATABASE_URL="postgresql://user:password@localhost:5432/agent_trust?schema=public"
PORT=3000

# RSA key pair for signing certificates (run "npm run generate-keys" to create)
STATION_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\n...\n-----END PRIVATE KEY-----"
STATION_PUBLIC_KEY="-----BEGIN PUBLIC KEY-----\n...\n-----END PUBLIC KEY-----"

# Certificate expiry in seconds (default: 300 = 5 minutes)
CERTIFICATE_EXPIRY_SECONDS=300
