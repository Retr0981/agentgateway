{
  "name": "agenttrust",
  "version": "1.0.0",
  "description": "The trust layer for the AI agent economy. Reputation scoring, staking, vouching, and cryptographic certificates for AI agents.",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn src/index.ts",
    "build": "tsc && npm run copy-public",
    "copy-public": "node -e \"const fs=require('fs');const path=require('path');function copyDir(s,d){fs.mkdirSync(d,{recursive:true});for(const f of fs.readdirSync(s)){const sp=path.join(s,f),dp=path.join(d,f);fs.statSync(sp).isDirectory()?copyDir(sp,dp):fs.copyFileSync(sp,dp)}}copyDir('src/public','dist/public')\"",
    "start": "node dist/index.js",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "generate-keys": "ts-node src/utils/keys.ts"
  },
  "keywords": [
    "ai",
    "agent",
    "trust",
    "verification",
    "reputation",
    "staking",
    "vouching",
    "certificate",
    "jwt",
    "gateway",
    "middleware",
    "captcha-alternative",
    "ai-agents",
    "machine-to-machine"
  ],
  "author": "AgentTrust",
  "license": "MIT",
  "homepage": "https://github.com/mmsadek96/agentgateway#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/mmsadek96/agentgateway.git"
  },
  "bugs": {
    "url": "https://github.com/mmsadek96/agentgateway/issues"
  },
  "dependencies": {
    "@prisma/client": "^5.10.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "jsonwebtoken": "^9.0.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.11.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^9.0.7",
    "prisma": "^5.10.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
